{
  "name": "glob-observable",
  "version": "0.1.0",
  "description": "Observable-based glob",
  "repository": "shinnn/glob-observable",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "pretest": "eslint --fix --format=codeframe index.js test.js",
    "test": "nyc node --throw-deprecation --harmony_async_await test.js"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords": [
    "glob",
    "file",
    "files",
    "pattern",
    "match",
    "matching",
    "path",
